--langdef=vhdl
--langmap=vhdl:.vhd
--regex-vhdl=/^[ \t]*use[ \t]+([^ ]+);/\1/l,package/
--regex-vhdl=/^[ \t]*entity[ \t]+([^ ]+) is/\1/e,module/
--regex-vhdl=/^[ \t]*signal[ \t]*([^ ]+)/\1/s,register/
--regex-vhdl=/^[ \t]*type[ \t]*([^ ]+)/\1/t,typedef/
--regex-vhdl=/^[ \t]*constant[ \t]*([^ ]+)/\1/c,constant/
--regex-vhdl=/^[ \t]*([^ \t:]+)[ \t]*:[ \t]*process[ \t]*\(/\1/p,block/
--regex-vhdl=/^[ \t]*([^ ]+)[ \t]*:[ \t]*(in|out)/\1/o,port/i